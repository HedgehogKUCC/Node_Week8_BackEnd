{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "tsc": "del-cli --force ./dist && tsc",
    "swagger:dev": "cross-env NODE_ENV=development node ./dist/swagger.js",
    "swagger:prod": "cross-env NODE_ENV=production node ./dist/swagger.js",
    "dev": "cross-env NODE_ENV=development nodemon ./dist/api/index.js",
    "dev:compile": "npm run tsc && npm run swagger:dev && npm run dev",
    "prod": "cross-env NODE_ENV=production nodemon ./dist/api/index.js",
    "prod:compile": "npm run tsc && npm run swagger:prod && npm run prod",
    "prod:build": "npm run tsc && npm run swagger:prod"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "dotenv": "16.0.0",
    "express": "4.16.4",
    "image-size": "1.0.1",
    "imgur": "2.2.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.3.2",
    "morgan": "1.9.1",
    "multer": "1.4.4",
    "swagger-ui-express": "4.6.2",
    "tslib": "2.4.0",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "9.0.1",
    "@types/morgan": "1.9.4",
    "@types/multer": "1.4.7",
    "@types/node": "18.13.0",
    "@types/swagger-ui-express": "4.1.3",
    "@types/validator": "13.7.12",
    "cross-env": "7.0.3",
    "del-cli": "5.0.0",
    "nodemon": "2.0.16",
    "swagger-autogen": "2.23.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
